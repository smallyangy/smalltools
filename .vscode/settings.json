{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.fixAll.stylelint": true
     },
     "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript"
    ],
    "eslint.alwaysShowStatus": true,
    "stylelint.enable": true,
    "stylelint.validate": [
        "css",
        "less",
        "postcss",
        "scss",
        "vue",
        "sass"
    ]
}
